= form_for(@admin_table) do |f|
  - if @admin_table.errors.any? 
    #error_explanation  
      %h2= pluralize(@admin_table.errors.count, "error") 
      "prohibited this rus_message from being saved:"
    %ul - @admin_table.errors.full_messages.each do |msg| 
    %li= msg

  #field
    = f.label :Tablename
    %br

    = f.text_field :name
    
  #actions
    = f.submit
